<kpro>
<!-- 
This xml consists of two components.  The first are teleport actions, which are items that reference C# components in kScripts.dll. 
The second component is the teleport object, which informs kCustomRadial which actions to load into the radial dial of the teleport object.
-->


<!-- Teleport Actions -->
	
<append xpath="/items">
	<item name="KProGoHome">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="KProTeleportProtocol" tiered="false"> 
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="GoHome, Mods" command="KProGoHome"/>
		</effect_group>
	</item>
	
	<item name="KProSetHome">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="KProTeleportProtocol" tiered="false"> 
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="SetHome, Mods" command="KProSetHome"/>
		</effect_group>
	</item>
	
	<item name="KProReturn">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="KProTeleportProtocol" tiered="false"> 
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="Return, Mods" command="KProSetHome"/>
		</effect_group>
	</item>

	<item name="KProSetWaypoint1">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="KProTeleportProtocol" tiered="false">
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="SetWaypoint, Mods" command="waypoint1"/>
		</effect_group>
	</item>

	<item name="KProGoWaypoint1">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="KProTeleportProtocol" tiered="false">
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="GoWaypoint, Mods" command="waypoint1"/>
		</effect_group>
	</item>

	<item name="KProSetWaypoint2">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="KProTeleportProtocol" tiered="false">
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="SetWaypoint, Mods" command="waypoint2"/>
		</effect_group>
	</item>

	<item name="KProGoWaypoint2">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="KProTeleportProtocol" tiered="false">
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="GoWaypoint, Mods" command="waypoint2"/>
		</effect_group>
	</item>

	<item name="KProTest">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="KProTeleportProtocol" tiered="false">
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="Test, Mods" command="waypoint2"/>
		</effect_group>
	</item>
<!--  Teleport Object>  -->
	

		<item name="KTeleport">
			<property name="Extends" value="gunBowT0PrimitiveBow"/>
			<property name="RadialItems" value="KProGoHome,KProReturn,KProSetHome,KProSetWaypoint1,KProGoWaypoint1,KProSetWaypoint2,KProGoWaypoint2, KProTest"/>
			<property name="MaxZedsAtWaypoint" value="3"/>
			<property name="ZombieTypeAtWaypoint" value="animalZombieDog"/>
			<property name="BasePercentChanceOfConsequence" value="5"/>
			<property name="AverageSpawnDistance" value="3"/>
			<property name="AverageDisplacementDistance" value="20"/>
		</item>
</append>

</kpro>