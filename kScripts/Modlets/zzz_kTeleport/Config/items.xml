<kpro>

<append xpath="/items">

<item name="KProGoHome">
	<property name="Extends" value="ammoArrowStone"/>
	<effect_group name="ammoArrowStone" tiered="false"> <!-- ammoArrowStone -->
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="GoHome, Mods" command="KProGoHome"/>
	</effect_group>
</item>

<item name="KProSetHome">
	<property name="Extends" value="ammoArrowStone"/>
	<effect_group name="ammoArrowStone" tiered="false"> <!-- ammoArrowStone -->
		<triggered_effect trigger="onSelfPrimaryActionEnd" action="SetHome, Mods" command="KProSetHome"/>
	</effect_group>
</item>

	<item name="KProReturn">
		<property name="Extends" value="ammoArrowStone"/>
		<effect_group name="ammoArrowStone" tiered="false"> <!-- ammoArrowStone -->
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="Return, Mods" command="KProSetHome"/>
		</effect_group>
	</item>


<item name="gunTeleport">
	<property name="Extends" value="gunBowT0PrimitiveBow"/>
	<property class="Action0">
		<property name="Magazine_items" value="KProCustomRadial,KProGoHome,KProReturn,KProSetHome"/>
		<!-- <property name="Reload_time" value=".9"/> -->
	</property>
	
		
</item>

</append>






</kpro>